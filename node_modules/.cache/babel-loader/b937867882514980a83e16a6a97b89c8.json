A,CAAA,QAAA,KAAA,CAAA,EAAA,QAAA,EAAA;AACA,UAAA,GAAA,GAAA,CAAA,CAAA,OAAA,GAAA,QAAA,EAAA;AACA,UAAA,IAAA,GAAA,CAAA,CAAA,WAAA,EAAA;;AACA,UAAA,KAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,QAAA,KAAA,GAAA,MAAA,KAAA;AACA;;AACA,UAAA,GAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,QAAA,GAAA,GAAA,MAAA,GAAA;AACA;;AACA,aAAA,CAAA,IAAA,EAAA,KAAA,EAAA,GAAA,EAAA,IAAA,CAAA,GAAA,CAAA;AACA,KAbA;AAcA,IAAA,YAdA,0BAcA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,UAAA;;AACA,UAAA,KAAA,UAAA,IAAA,KAAA,OAAA,CAAA,KAAA,EAAA;AACA,aAAA,YAAA,GAAA,wBAAA;AACA,OAFA,MAEA;AACA,YAAA,KAAA,KAAA,IAAA,KAAA,aAAA,EAAA;AACA,eAAA,YAAA,GAAA,oBAAA;AACA,SAFA,MAEA;AACA,eAAA,OAAA,CAAA,KAAA,GAAA,KAAA,KAAA;AACA,eAAA,aAAA;AACA;AACA;AACA,KA1BA;AA2BA,IAAA,aA3BA,2BA2BA;AAAA;;AACA,WAAA,OAAA,CAAA,YAAA,GAAA,KAAA,UAAA,CAAA,KAAA,OAAA,CAAA,YAAA,CAAA;AACA,WAAA,OAAA,CAAA,OAAA,GAAA,KAAA,OAAA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,mEAAA,EAAA,KAAA,OAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,MAAA,IAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,YAAA,GAAA,IAAA;AACA,UAAA,MAAA,CAAA,OAAA,GAAA,sBAAA;AACA,iBAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,YAAA,IAAA,EAAA;AAAA,WAAA,CAAA;AACA,SAJA,MAIA;AACA,UAAA,MAAA,CAAA,YAAA,GAAA,kBAAA;AACA;AACA,OARA,EAQA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OAVA;AAWA;AAzCA;AA/BA,CAAA","sourcesContent":["<script>\r\nimport Header from './shared/HeaderBarDetail';\r\nimport TheMask from 'vue-the-mask';\r\n\r\nexport default {\r\n    components: {\r\n        Header,\r\n        TheMask,\r\n    },\r\n    data() {\r\n        return {\r\n            usuario: {},\r\n            cliente: {},\r\n            message: null,\r\n            messageError: null,\r\n            alterarSenha: false,\r\n            senhaAtual: '',\r\n            senha: '',\r\n            confirmaSenha: '',\r\n            apelido: ''\r\n        }\r\n    },\r\n    created(){\r\n        this.usuario = this.$root.usuario;\r\n        if(this.usuario == null){\r\n            return this.$router.push({ name: 'login' })\r\n        }\r\n        this.$http.post('https://rakgameshop-prod.herokuapp.com/api/cliente/buscarcliente', this.usuario).then(res => {\r\n            this.cliente = res.body;\r\n            this.cliente.dataNascimento = this.formatDate(this.cliente.dataNascimento);\r\n            this.apelido = this.cliente.apelido;\r\n        }, res => {\r\n            console.log(res);\r\n        });\r\n    },\r\n    methods: {\r\n        formatDate(date) {\r\n            let d = new Date(date);\r\n            let month = (d.getMonth() + 1).toString();\r\n            let day = d.getDate().toString();\r\n            let year = d.getFullYear();\r\n            if (month.length < 2) {\r\n                month = '0' + month;\r\n            }\r\n            if (day.length < 2) {\r\n                day = '0' + day;\r\n            }\r\n            return [year, month, day].join('-');\r\n        },\r\n        validarSenha(){\r\n            console.log(this.senhaAtual)\r\n            if(this.senhaAtual != this.usuario.senha){\r\n                this.messageError = \"Senha Atual Incorreta!\";\r\n            }else{\r\n                if(this.senha != this.confirmaSenha){\r\n                    this.messageError = \"Senhas Diferentes!\";\r\n                }else{\r\n                    this.usuario.senha = this.senha;\r\n                    this.salvarCliente();\r\n                }\r\n            }\r\n        },\r\n        salvarCliente() {\r\n            this.usuario.dataCadastro = this.formatDate(this.usuario.dataCadastro);\r\n            this.cliente.usuario = this.usuario;\r\n            this.$http.post('https://rakgameshop-prod.herokuapp.com/api/cliente/AlterarCliente', this.cliente).then(res => {\r\n                if (res.status == 200) {\r\n                    this.messageError = null;\r\n                    this.message = \"Alteração concluída!\";\r\n                     return this.$router.push({ name: 'dadosCliente' })\r\n                } else {\r\n                    this.messageError = \"Erro ao Alterar!\";\r\n                }\r\n            }, res => {\r\n                console.log(res);\r\n            });\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n\r\n<template>\r\n    <div>\r\n        <Header spanLinks=\"true\" />\r\n        <div class=\"container-fluid\">\r\n            <div class=\"row justify-content-center\">\r\n                <div class=\"col-7\">\r\n                    <form class=\"form-new\">\r\n                        <div class=\"row justify-content-center mt-3\">\r\n                            <div class=\"container mt-5\">\r\n                                <form class=\"form-new\">\r\n                                    <p style=\"font-size: 20px; margin-top: 20px\">{{apelido}}</p>\r\n                                </form>\r\n                            </div>\r\n                            <div class=\"col-3\">\r\n                                <h5>Alterar Dados</h5>\r\n                            </div>\r\n                            <div v-if=\"message\" class=\"alert alert-success alert-dismissible fade show\" role=\"alert\">\r\n                                {{ message }}\r\n                                <button type=\"button\" @click=\"message = null\" data-dismiss=\"alert\"\r\n                                    aria-label=\"Close\">\r\n                                    <span aria-hidden=\"true\">&times;</span>\r\n                                </button>\r\n                            </div>\r\n                            <div v-if=\"messageError\" class=\"alert alert-danger alert-dismissible fade show\"\r\n                                role=\"alert\">\r\n                                {{ messageError }}\r\n                                <button type=\"button\" @click=\"messageError = null\" data-dismiss=\"alert\"\r\n                                    aria-label=\"Close\">\r\n                                    <span aria-hidden=\"true\">&times;</span>\r\n                                </button>\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"row\">\r\n                            <div class=\"mb-3 mt-2\">\r\n                                <label class=\"form-label\">Nome Completo</label>\r\n                                <input v-model=\"cliente.nome\" type=\"text\" class=\"form-control\" id=\"formNome\">\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"row\">\r\n                            <div class=\"mb-3 mt-2\">\r\n                                <label class=\"form-label\">Apelido</label>\r\n                                <input v-model=\"cliente.apelido\" type=\"text\" class=\"form-control\" id=\"formApelido\">\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"row\">\r\n                            <div class=\"mb-3 mt-2\">\r\n                                <label class=\"form-label\">E-Mail</label>\r\n                                <input v-model=\"usuario.email\" type=\"text\" class=\"form-control\" id=\"formEmail\">\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"row\">\r\n                            <div class=\"mb-3 mt-2\">\r\n                                <label class=\"form-label\">Data de Nascimento</label>\r\n                                <input v-model=\"cliente.dataNascimento\" type=\"date\" class=\"form-control\"\r\n                                    id=\"formDataNascimento\">\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"row\">\r\n                            <div class=\"mb-3 mt-2\">\r\n                                <label class=\"form-label\">CPF</label>\r\n                                <the-mask />\r\n                                <input v-model=\"cliente.cpf\" type=\"text\" class=\"form-control\" id=\"formCpf\"\r\n                                    v-mask=\"'###.###.###-##'\">\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"row\">\r\n                            <div class=\"mb-3 mt-2\">\r\n                                <label class=\"form-label\">Telefone</label>\r\n                                <input v-model=\"cliente.telefone\" type=\"text\" class=\"form-control\" id=\"formTelefone\"\r\n                                    v-mask=\"['(##) ####-####', '(##) #####-####']\">\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"row\">\r\n                            <div class=\"mb-3 mt-4\">\r\n                                <label @click=\"alterarSenha = !alterarSenha\" style=\"cursor: pointer\" class=\"form-label\"><u>Alterar Senha</u></label>\r\n                            </div>\r\n                        </div>\r\n                         <div v-if=\"alterarSenha\" class=\"row\">\r\n                            <div class=\"mb-3 mt-2\">\r\n                                <label class=\"form-label\">Senha Atual</label>\r\n                                <input v-model=\"senhaAtual\" type=\"password\" class=\"form-control\" id=\"formSenhaAtual\">\r\n                            </div>\r\n                        </div>\r\n                        <div v-if=\"alterarSenha\" class=\"row\">\r\n                            <div class=\"mb-3 mt-2\">\r\n                                <label class=\"form-label\">Nova Senha</label>\r\n                                <input v-model=\"senha\" type=\"password\" class=\"form-control\" id=\"formSenha\">\r\n                            </div>\r\n                        </div>\r\n                        <div v-if=\"alterarSenha\" class=\"row\">\r\n                            <div class=\"mb-3 mt-2\">\r\n                                <label class=\"form-label\">Confirmar Senha</label>\r\n                                <input v-model=\"confirmaSenha\" type=\"password\" class=\"form-control\" id=\"formSenhaConfirm\">\r\n                            </div>\r\n                        </div>\r\n                        <div v-if=\"alterarSenha\" class=\"row\">\r\n                            <div class=\"mb-3 mt-2\">\r\n                                <button class=\"btn btn-md btt-outline\" @click=\"validarSenha()\">Alterar</button>\r\n                            </div>\r\n                        </div>\r\n                        <div v-if=\"message\" class=\"alert alert-success alert-dismissible fade show\" role=\"alert\">\r\n                            {{ message }}\r\n                            <button type=\"button\" @click=\"message = null\" data-dismiss=\"alert\"\r\n                                aria-label=\"Close\">\r\n                                <span aria-hidden=\"true\">&times;</span>\r\n                            </button>\r\n                        </div>\r\n                        <div v-if=\"messageError\" class=\"alert alert-danger alert-dismissible fade show\"\r\n                            role=\"alert\">\r\n                            {{ messageError }}\r\n                            <button type=\"button\" @click=\"messageError = null\" data-dismiss=\"alert\"\r\n                                aria-label=\"Close\">\r\n                                <span aria-hidden=\"true\">&times;</span>\r\n                            </button>\r\n                        </div>\r\n                        <hr>\r\n                        <div class=\"row justify-content-end\">\r\n                            <div class=\"mb-3 mt-4\">\r\n                                <button class=\"btn btn-md btt-enviar\" @click=\"salvarCliente()\">Enviar</button>\r\n                            </div>\r\n                        </div>\r\n                    </form>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<style>\r\n.form-new {\r\n    color: white;\r\n}\r\n\r\n.btt-enviar {\r\n    color: #ffff;\r\n    background-color: #340E80;\r\n}\r\n\r\n.btt-enviar:hover {\r\n    color: #ffff;\r\n    background-color: #7e64b3;\r\n}\r\n\r\n.btt-outline{\r\n    color: #ffff;\r\n    border: 1px solid #340E80;\r\n}\r\n\r\n.btt-outline:hover{\r\n    color: #ffff;\r\n    border: 1px solid #340E80;\r\n    background-color: #413f3f;\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}